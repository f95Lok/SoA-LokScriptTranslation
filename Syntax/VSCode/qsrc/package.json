{
    "name": "qsrc",
    "displayName": "qsrc",
    "description": "QSP grammar",
    "version": "0.2.9",
    "publisher": "Xorgroth",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qsp",
            "aliases": ["QSP", "qsp"],
            "extensions": [".qsrc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "qsp",
            "scopeName": "text.qsp.qsrc",
            "path": "./syntaxes/qsp.tmLanguage.json"
        }]
    }
}
